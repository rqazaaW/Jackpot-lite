if game.PlaceId == 14172236627 then
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "[ðŸ’Ž] Lite Hub | Jackpote Lite", HidePremium = false, IntroText = "[ðŸ’Ž] Lite Hub", SaveConfig = true, ConfigFolder = "Lite Hub Config"})


--Values
_G.autoSell = true

--Functions

function autoSell()
  while wait() do
      game:GetService("ReplicatedStorage").Remotes.Sell:FireServer(unpack(args))
      wait(.0001)
  end
 end

--Tabs
local MainTab = Window:MakeTab({
    Name = "Main",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

local AutoFunctions = Window:MakeTab({
    Name = "Autofarm",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})
local CreditsTab = Window:MakeTab({
    Name = "Credits",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

--Buttons
AutoFunctions:AddButton({
    Name = "Auto Open [Dominus]",
    Callback = function()
getgenv().farmer = true;

while wait() do
   if getgenv().farmer == true then 
game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("OpenCrate"):FireServer("Dominus")

 end
end
end
})
AutoFunctions:AddButton({
    Name = "Auto Open [Mythical]",
    Callback = function()
getgenv().farmer = true;

while wait() do
   if getgenv().farmer == true then 
game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("OpenCrate"):FireServer("Mythical")

 end
end
end
})
MainTab:AddButton({
	Name = "Infinite Jump",
	Callback = function()
      		local InfiniteJumpEnabled = true
game:GetService("UserInputService").JumpRequest:connect(function()
	if InfiniteJumpEnabled then
		game:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass'Humanoid':ChangeState("Jumping")
	end
end)
  	end    
})
MainTab:AddButton({
	Name = "Walkspeed Boost",
	Callback = function()
      		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 100
  	end    
})
AutoFunctions:AddButton({
    Name = "Auto Open [Fedora]",
    Callback = function()

getgenv().farmer = true;

while wait() do
   if getgenv().farmer == true then 
game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("OpenCrate"):FireServer("Fedora")

 end
end
end
})
AutoFunctions:AddButton({
    Name = "Auto Sell [Every 20 minutes]",
    Callback = function()
              while wait(1200) do
game:GetService("ReplicatedStorage").Remotes.Sell:FireServer("All", "", "")
end
      end    
})
AutoFunctions:AddButton({
    Name = "Auto Sell [Instant]",
    Callback = function()
              while wait() do
game:GetService("ReplicatedStorage").Remotes.Sell:FireServer("All", "", "")
end
      end    
})

AutoFunctions:AddToggle({
    Name = "Auto Click [Case Bux]",
    Default = false,
    Callback = function(AutoClick)
            if AutoClick then
            _G.loop = true
            while _G.loop == true do wait()
                game:GetService("ReplicatedStorage").Remotes.Click:FireServer()
            end 
        else
            _G.loop = false
            while _G.loop == true do wait()
                game:GetService("ReplicatedStorage").Remotes.Click:FireServer()
        end
      end
    end    
})

